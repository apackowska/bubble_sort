   "source": [
    "def sort (list):\n",
    "    n = len(list)\n",
    "    while n>0:\n",
    "        for i in range(0, n-1):\n",
    "            if list[i]> list[i+1]:\n",
    "                list [i], list [i+1] = list[i+1], list[i]\n",
    "        n-=1\n",
    "        \n",
    "    return list"
   ]
